<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="lt.eliga.u2020">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/application_name"
        android:theme="@style/Theme.U2020"
        android:name=".U2020App"
        >
        <activity
            android:name=".ui.activity_main.MainActivity"
            android:label="@string/application_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity_about.AboutActivity"
            android:label="@string/image_activity_title"/>
    </application>
</manifest>
